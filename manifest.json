{
  "name": "BigCommerce Coupon Importer",
  "author": "Billy Shih original by Setfive Consulting LLC",
  "description": "Adds an Import button to the BigCommerce coupon codes screen.",
  "version": "1.1",
  "manifest_version": 2,
  "background": { "page": "background.html" },
  "content_scripts": [
	{
      "matches": ["https://*/*"],
      "js": ["jquery-1.7.2.min.js", "jquery.jstree.js", "main.js"]
	}
  ],
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications",
    "tabs",
    "*://*/*"
  ]
}
